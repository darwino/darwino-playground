        "produces": [
          "application\/json"
        ],
        "description":"return the list of registered JSQL queries",
        "responses": {
          "200": {
            "description":"OK. Indicates that the request was received successfully."
          },
          "404": {
            "description":"Not found"
          }
        },
        "parameters": [

        ],
        "tags": [
          "JSQL Queries"
        ]
      }
    },
