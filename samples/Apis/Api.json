{
  "basePath":"\/playground",
  "paths": {
    "\/$darwino-discovery\/apis": {
      "get": {
        "description":"Get the list of available APIs\n",
        "responses": {
          "200": {
            "description":"Successful response"
          }
        },
        "parameters": [
                    {
            "in":"query",
            "name":"format",
            "description":"Rendering format",
            "type":"string",
            "required":false,
            "enum": [
              "json",
              "yaml"
            ]
          }
        ],
        "tags": [
          "API Discovery"
        ]
      }
    }
  },
  "host":"localplayground.darwino.com",
  "schemes": [
    "http",
    "https"
  ],
  "securityDefinitions": {
    "basicAuth": {
      "description":"HTTP Basic Authentication. Works over 'HTTP' and 'HTTPS'",
      "type":"basic"
    }
  },
  "swagger":"2.0",
  "info": {
    "description":"Discover the available REST APIs",
    "title":"API Discovery",
    "version":"2.2.0"
  }
}